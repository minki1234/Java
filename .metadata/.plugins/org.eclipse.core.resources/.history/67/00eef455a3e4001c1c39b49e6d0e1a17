package com.example.rorro.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;

@Controller
public class MainController {
	

	@GetMapping("/")
	public String home() {
		return "index";
	}
	
	@GetMapping("/another")
	public String anotherPage() {
		return "another";
	}
}